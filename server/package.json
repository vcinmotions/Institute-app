{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "dist/setup-tenant.js",
  "pkg": {
    "scripts": [
      "dist/**/*.js",
      "utils/**/*.js"
    ],
    "assets": [
      "prisma/institute/schema.prisma"
    ]
  },
  "scripts": {
    "build": "tsc && cpx \"prisma-client/generated/**/*\" dist/prisma-client/generated && cpx \"prisma/**/*\" dist/prisma && cpx \"prisma/central/.env\" dist/prisma/central && cpx \"prisma/institute/.env\" dist/prisma/institute && cpx \".env\" dist && cpx \".env.dev\" dist && cpx \".env.prod\" dist && cpx \".env.dev\" dist/prisma/central && cpx \".env.prod\" dist/prisma/central",
    "dev": "electron .",
    "start-prod": "node dist/server.js",
    "start": "ts-node-dev --respawn server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed:labs": "npx ts-node src/scripts/seed-labs-all-tenants.ts",
    "create-centralDB": "npx ts-node src/utils/createCentralDB.ts",
    "migrate-all": "npx ts-node src/scripts/migrate-all-tenants.ts",
    "generate:central": "npx prisma generate --schema=prisma/central/schema.prisma",
    "generate:tenant": "npx prisma generate --schema=prisma/tenant/schema.prisma",
    "generate:central-prod": "npx prisma generate --schema=prisma/central/schema.sqlite.prisma",
    "generate:tenant-prod": "npx prisma generate --schema=prisma/tenant/schema.sqlite.prisma",
    "migrate:central": "prisma migrate dev --schema=prisma/central/schema.prisma",
    "migrate:tenant": "prisma migrate dev --schema=prisma/tenant/schema.prisma",
    "deploy:central": "cross-env DATABASE_URL=$CENTRAL_DATABASE_URL prisma migrate deploy --schema=prisma/central/schema.prisma",
    "deploy:tenant": "cross-env DATABASE_URL=$TENANT_DATABASE_URL prisma migrate deploy --schema=prisma/tenant/schema.prisma",
    "followup-cron": "ts-node src/cronJobs/cron-jon-runner.ts",
    "setup:sample": "ts-node scripts/setup-sample-tenant.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@reduxjs/toolkit": "^2.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "rate-limit-redis": "^4.2.3",
    "react-redux": "^9.2.0",
    "resend": "^6.1.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.4",
    "@types/pdfkit": "^0.17.3",
    "@types/pg": "^8.16.0",
    "@types/yargs": "^17.0.33",
    "cpx": "^1.5.0",
    "cross-env": "^10.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
